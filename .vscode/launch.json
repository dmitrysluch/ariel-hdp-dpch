{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "debugpy",
      "request": "launch",
      "name": "[USE ONLY IN DEVCONTAINER] Launch dpch FastAPI app",
      "module": "uvicorn",
      "args": [
        "dpch.server.proxy:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8000"
      ],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
      "jinja": true,
      "justMyCode": true,
      "console": "integratedTerminal"
    }
  ]
}